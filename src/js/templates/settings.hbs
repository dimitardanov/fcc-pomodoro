<button
  type="button" class="left fa fa-backward"
  data-target="{{prevBtnTarget}}"
  title="{{prevBtnTitle}}"
  >
</button>
<button
  type="button" class="right fa fa-forward"
  data-target="{{nextBtnTarget}}"
  title="{{nextBtnTitle}}"
  >
</button>
<p id="timer"><span id="minutes">{{minutes}}</span>:<span id="seconds">{{seconds}}</span></p>
<p id="message">{{message}}</p>
<ul class="indicators">
  {{#each order}}
    <li
      data-target="{{@index}}"
      title="{{this}}"
      ></li>
  {{/each}}
</ul>
